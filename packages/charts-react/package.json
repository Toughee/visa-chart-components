{
  "name": "@visa/charts-react",
  "version": "2.0.2",
  "description": "A react wrapper built with Stencil's bindings for the visa-charts-lib stencil.js based component library. ",
  "license": "SEE LICENCE IN LICENSE",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visa/visa-chart-components.git",
    "directory": "/packages/charts-react"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Visa Data Visualization Team",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
    "@visa/bar-chart": "^4.0.2",
    "@visa/bivariate-mapbox-map": "^5.0.2",
    "@visa/circle-packing": "^5.0.2",
    "@visa/clustered-bar-chart": "^4.0.2",
    "@visa/clustered-force-layout": "^4.0.2",
    "@visa/dumbbell-plot": "^5.0.1",
    "@visa/group-charts": "^3.1.8",
    "@visa/heat-map": "^4.0.2",
    "@visa/level-indicator": "^4.0.2",
    "@visa/line-chart": "^4.0.2",
    "@visa/parallel-plot": "^4.0.2",
    "@visa/pareto-chart": "^4.0.2",
    "@visa/pie-chart": "^4.0.2",
    "@visa/scatter-plot": "^5.0.1",
    "@visa/stacked-bar-chart": "^4.0.2",
    "@visa/visa-charts-data-table": "^2.0.2",
    "@visa/visa-charts-utils": "^4.1.1",
    "@visa/world-map": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.3.4000"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
